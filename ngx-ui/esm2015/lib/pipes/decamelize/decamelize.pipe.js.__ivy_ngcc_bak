import { Pipe } from '@angular/core';
export class DecamelizePipe {
    transform(input) {
        if (!input)
            return '';
        const s = input.toString();
        return s.charAt(0).toUpperCase() + s.substr(1).replace(/[A-Z]/g, ' $&');
    }
}
DecamelizePipe.decorators = [
    { type: Pipe, args: [{
                name: 'decamelize'
            },] }
];
