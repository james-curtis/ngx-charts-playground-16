import { Pipe } from '@angular/core';
export class CammelToSnakePipe {
    transform(input) {
        if (!input)
            return '';
        const str = input.toString();
        return str
            .split(/(?=[A-Z])/)
            .join('_')
            .toLowerCase();
    }
}
CammelToSnakePipe.decorators = [
    { type: Pipe, args: [{ name: 'cammeltosnake' },] }
];
