import { Pipe } from '@angular/core';
import { jsonToTree } from './json-to-tree.util';
export class JSONTreePipe {
    transform(input) {
        return [jsonToTree(input)];
    }
}
JSONTreePipe.decorators = [
    { type: Pipe, args: [{
                name: 'jsonTree'
            },] }
];
//# sourceMappingURL=data:application/json;base64,