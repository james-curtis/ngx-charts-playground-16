import { Directive } from '@angular/core';
export class CardBodyDirective {
}
CardBodyDirective.decorators = [
    { type: Directive, args: [{
                selector: 'ngx-card-body, [ngxCardBody], [ngx-card-body]',
                host: { class: 'ngx-card-body' }
            },] }
];
