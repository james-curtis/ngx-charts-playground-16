import { Directive, TemplateRef } from '@angular/core';
export class SelectOptionInputTemplateDirective {
    constructor(template) {
        this.template = template;
    }
}
SelectOptionInputTemplateDirective.decorators = [
    { type: Directive, args: [{
                exportAs: 'ngxSelectOptionInputTemplate',
                selector: '[ngx-select-option-input-template]'
            },] }
];
SelectOptionInputTemplateDirective.ctorParameters = () => [
    { type: TemplateRef }
];
