import { Component, ChangeDetectionStrategy, ViewEncapsulation, Input, ChangeDetectorRef, TemplateRef, HostBinding } from '@angular/core';
import { coerceNumberProperty } from '@angular/cdk/coercion';
import { ProgressSpinnerMode } from './progress-spinner-mode.enum';
import { id } from '../../utils/id/id.util';
export var SpinnerAppearance;
(function (SpinnerAppearance) {
    SpinnerAppearance["Default"] = "default";
    SpinnerAppearance["Icon"] = "icon";
})(SpinnerAppearance || (SpinnerAppearance = {}));
export class ProgressSpinnerComponent {
    constructor(_cdr) {
        this._cdr = _cdr;
        this.mode = ProgressSpinnerMode.Indeterminate;
        this.color = '#1483FF';
        this.failStatusColor = '#FF4514';
        this.appearance = SpinnerAppearance.Default;
        this.uid = id();
        this.ProgressSpinnerMode = ProgressSpinnerMode;
        this._value = 0;
        this._total = 100;
        this._diameter = 100;
        this._strokeWidth = 3;
        this._indeterminate = {
            value: 50,
            total: 100
        };
    }
    get value() {
        return this._value;
    }
    set value(v) {
        this._value = coerceNumberProperty(v);
        this._cdr.markForCheck();
    }
    get total() {
        return this._total;
    }
    set total(t) {
        this._total = t;
        this._cdr.markForCheck();
    }
    get diameter() {
        return this._diameter;
    }
    set diameter(d) {
        this._diameter = coerceNumberProperty(d);
        this._cdr.markForCheck();
    }
    get strokeWidth() {
        return this._strokeWidth;
    }
    set strokeWidth(s) {
        this._strokeWidth = coerceNumberProperty(s);
        this._cdr.markForCheck();
    }
    get radius() {
        return this.diameter / 2;
    }
    get strokeDasharray() {
        return `${this._circumference} ${this._circumference}`;
    }
    get strokeDashoffset() {
        return this._circumference - (this.percentage / 100) * this._circumference;
    }
    get percentage() {
        return (100 / this._modeTotal) * this._modeValue;
    }
    get isComplete() {
        return this.value === this.total;
    }
    get indeterminateClass() {
        return this.mode === ProgressSpinnerMode.Indeterminate;
    }
    get spinnerColor() {
        return this.isComplete && this.isFailure ? this.failStatusColor : this.color;
    }
    get showIcon() {
        return this.appearance === SpinnerAppearance.Icon;
    }
    get _circumference() {
        return this.radius * 2 * Math.PI;
    }
    get _modeValue() {
        return this.mode === ProgressSpinnerMode.Determinate || this.isComplete ? this.value : this._indeterminate.value;
    }
    get _modeTotal() {
        return this.mode === ProgressSpinnerMode.Determinate || this.isComplete ? this.total : this._indeterminate.total;
    }
}
