import { Directive } from '@angular/core';
export class DrawerContainerDirective {
}
DrawerContainerDirective.decorators = [
    { type: Directive, args: [{
                exportAs: 'drawerContainer',
                selector: '[drawerContainer]',
                host: {
                    '[style.position]': '"relative"',
                    '[style.overflow]': '"hidden"'
                }
            },] }
];
