import { Directive, HostBinding } from '@angular/core';
export class LargeFormatDialogSubTabsDirective {
    constructor() {
        this.hostClass = true;
    }
}
LargeFormatDialogSubTabsDirective.decorators = [
    { type: Directive, args: [{
                selector: 'ngx-tabs[largeFormatDialogSubTabs]'
            },] }
];
LargeFormatDialogSubTabsDirective.propDecorators = {
    hostClass: [{ type: HostBinding, args: ['class.dialog-tabs--sub',] }]
};
