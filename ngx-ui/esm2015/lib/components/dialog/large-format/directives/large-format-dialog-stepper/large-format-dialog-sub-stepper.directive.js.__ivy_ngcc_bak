import { Directive, HostBinding } from '@angular/core';
export class LargeFormatDialogSubStepperDirective {
    constructor() {
        this.hostClass = true;
    }
}
LargeFormatDialogSubStepperDirective.decorators = [
    { type: Directive, args: [{
                selector: 'ngx-stepper[largeFormatDialogSubStepper]'
            },] }
];
LargeFormatDialogSubStepperDirective.propDecorators = {
    hostClass: [{ type: HostBinding, args: ['class.dialog-stepper--sub',] }]
};
