    var patternValidator = function (pattern) {
        return function (control) {
            // eslint-disable-next-line
            var valid = new RegExp(pattern).test(control.value);
            return valid ? null : { patternNotMatching: { value: control.value } };
        };
    };

    var PatternValidatorDirective = /** @class */ (function () {
