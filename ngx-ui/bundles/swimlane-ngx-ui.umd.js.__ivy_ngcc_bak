    var RadioButtonComponent = /** @class */ (function () {
        function RadioButtonComponent(cdr) {
            this.cdr = cdr;
            this.UNIQUE_ID = "ngx-radio-" + ++nextId$2;
            this.id = this.UNIQUE_ID;
            this.name = this.UNIQUE_ID;
            this.change = new i0.EventEmitter();
            this.blur = new i0.EventEmitter();
            this.focus = new i0.EventEmitter();
            this.groupDisabled = false;
            this._checked = false;
            this._value = false;
            this._disabled = false;
            this._tabindex = 0;
        }
        Object.defineProperty(RadioButtonComponent.prototype, "tabindex", {
            get: function () {
                return this._tabindex;
            },
            set: function (tabindex) {
                this._tabindex = coercion.coerceNumberProperty(tabindex);
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(RadioButtonComponent.prototype, "checked", {
            get: function () {
                return this._checked;
            },
            set: function (checked) {
                checked = coercion.coerceBooleanProperty(checked);
                if (this._checked !== checked) {
                    this._checked = checked;
                    this.onChangeCallback(this._value);
                }
                this.cdr.markForCheck();
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(RadioButtonComponent.prototype, "value", {
            get: function () {
                return this._value;
            },
            set: function (value) {
                if (this._value !== value) {
                    this._value = value;
                    this.onChangeCallback(this._value);
                }
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(RadioButtonComponent.prototype, "disabled", {
            get: function () {
