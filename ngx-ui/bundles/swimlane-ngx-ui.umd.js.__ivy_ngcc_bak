    var LoadingModule = /** @class */ (function () {
        function LoadingModule() {
        }
        return LoadingModule;
    }());
    LoadingModule.decorators = [
        { type: i0.NgModule, args: [{
                    declarations: [LoadingComponent],
                    providers: [InjectionService],
