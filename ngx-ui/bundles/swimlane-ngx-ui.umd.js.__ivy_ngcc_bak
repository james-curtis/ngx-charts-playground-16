    AlertService.ɵprov = i0__namespace.ɵɵdefineInjectable({ factory: function AlertService_Factory() { return new AlertService(i0__namespace.ɵɵinject(InjectionService), i0__namespace.ɵɵinject(OverlayService)); }, token: AlertService, providedIn: "root" });
