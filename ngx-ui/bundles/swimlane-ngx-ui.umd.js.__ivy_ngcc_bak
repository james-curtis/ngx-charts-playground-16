    function basisToValue(basis) {
        if (typeof basis === 'string') {
            return Number(basis.replace(/%/g, '').replace(/px/g, ''));
        }
        return basis;
    }

    function isPercent(basis) {
        var hasCalc = String(basis).indexOf('calc') > -1;
        return String(basis).indexOf('%') > -1 && !hasCalc;
    }

    function getMinMaxPct(minBasis, maxBasis, grow, shrink, baseBasisPct, basisToPx) {
        // minimum and maximum basis determined by max/min inputs
        var minBasisPct = isPercent(minBasis) ? basisToValue(minBasis) : basisToValue(minBasis) / basisToPx;
        var maxBasisPct = isPercent(maxBasis) ? basisToValue(maxBasis) : basisToValue(maxBasis) / basisToPx;
        // minimum and maximum basis determined by flex inputs
        minBasisPct = Math.max(minBasisPct || 0, shrink === '0' ? baseBasisPct : 0);
        maxBasisPct = Math.min(maxBasisPct || 100, grow === '0' ? baseBasisPct : 100);
        return [minBasisPct, maxBasisPct];
    }

    function resizeAreaBy(area, _delta, basisToPx) {
        var _a = __read(area.currentFlexParts, 3), grow = _a[0], shrink = _a[1], basis = _a[2];
        var isPct = isPercent(basis);
        var basisValue = basisToValue(basis);
        // get baseBasis in percent
        var baseBasis = area.initialFlexParts[2];
        var baseBasisPct = isPercent(baseBasis) ? basisToValue(baseBasis) : basisToValue(baseBasis) / basisToPx;
        // get basis in px and %
        var basisPx = isPct ? basisValue * basisToPx : basisValue;
        // determine which dir and calc the diff
        var newBasisPx = basisPx + _delta;
        var newBasisPct = newBasisPx / basisToPx;
        var _b = __read(getMinMaxPct(area.minBasis, area.maxBasis, grow, shrink, baseBasisPct, basisToPx), 2), minBasisPct = _b[0], maxBasisPct = _b[1];
        // obey max and min
        newBasisPct = Math.max(newBasisPct, minBasisPct);
        newBasisPct = Math.min(newBasisPct, maxBasisPct);
        // calculate new basis on px
        newBasisPx = newBasisPct * basisToPx;
        // update flexlayout
        area.updateBasis(isPct ? newBasisPct + '%' : newBasisPx + 'px');
        // return actual change in px
        return newBasisPx - basisPx;
    }

    var SplitDirective = /** @class */ (function () {
        function SplitDirective(elementRef) {
            this.elementRef = elementRef;
            this.direction = exports.SplitDirection.Row;
            this.subscriptions = [];
        }
        Object.defineProperty(SplitDirective.prototype, "rowCss", {
            get: function () {
                return this.direction === exports.SplitDirection.Row;
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(SplitDirective.prototype, "columnCss", {
            get: function () {
                return this.direction === exports.SplitDirection.Column;
            },
            enumerable: false,
            configurable: true
        });
        SplitDirective.prototype.ngAfterContentInit = function () {
            var _a, _b;
            var _this = this;
            (_a = this.subscriptions).push.apply(_a, __spreadArray([], __read(this.handles.map(function (d) { return d.drag.subscribe(function (ev) { return _this.onDrag(ev); }); }))));
            (_b = this.subscriptions).push.apply(_b, __spreadArray([], __read(this.handles.map(function (d) { return d.dblclick.subscribe(function () { return _this.onDblClick(); }); }))));
            this.updateHandles();
        };
        SplitDirective.prototype.ngOnChanges = function () {
            if (!this.direction) {
                this.direction = exports.SplitDirection.Row;
            }
            this.updateHandles();
        };
        SplitDirective.prototype.ngOnDestroy = function () {
            this.subscriptions.forEach(function (sub) {
                sub.unsubscribe();
            });
        };
        SplitDirective.prototype.resize = function (delta) {
            var basisToPx = (this.rowCss ? this.elementRef.nativeElement.clientWidth : this.elementRef.nativeElement.clientHeight) / 100;
            var areas = this.areas.toArray();
            // for now assuming splitter is after first area
            var _a = __read(areas), first = _a[0], rest = _a.slice(1);
            [first].forEach(function (area) { return (delta = resizeAreaBy(area, delta, basisToPx)); });
            // delta is distributed left to right
            rest.forEach(function (area) { return (delta += resizeAreaBy(area, -delta, basisToPx)); });
        };
        SplitDirective.prototype.updateHandles = function () {
            var _this = this;
            if (this.handles) {
                this.handles.forEach(function (d) { return (d.direction = _this.direction); });
            }
        };
        SplitDirective.prototype.onDblClick = function () {
            var basisToPx = (this.rowCss ? this.elementRef.nativeElement.clientWidth : this.elementRef.nativeElement.clientHeight) / 100;
            var area = this.areas.first;
            /* istanbul ignore if */
            if (!area)
                return;
            var _a = __read(area.currentFlexParts, 3), grow = _a[0], shrink = _a[1], basis = _a[2];
            var isPct = isPercent(basis);
            var basisValue = basisToValue(basis);
            // get basis in px and %
            var basisPx = isPct ? basisValue * basisToPx : basisValue;
            var basisPct = basisPx / basisToPx;
            // get baseBasis in percent
            var baseBasis = area.initialFlexParts[2];
            var baseBasisPct = basisToValue(baseBasis) / (isPercent(baseBasis) ? basisToPx : 1);
            var _b = __read(getMinMaxPct(area.minBasis, area.maxBasis, grow, shrink, baseBasisPct, basisToPx), 2), minBasisPct = _b[0], maxBasisPct = _b[1];
            // max and min deltas
            var deltaMin = basisPct - minBasisPct;
            var deltaMax = maxBasisPct - basisPct;
            var delta = deltaMin < deltaMax ? deltaMax : -deltaMin;
            var deltaPx = delta * basisToPx;
            this.resize(deltaPx);
        };
        SplitDirective.prototype.onDrag = function (_a) {
            var movementX = _a.movementX, movementY = _a.movementY;
            var deltaPx = this.direction === exports.SplitDirection.Row ? movementX : movementY;
            this.resize(deltaPx);
        };
        return SplitDirective;
    }());
