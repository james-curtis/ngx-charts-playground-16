    /**
     * Debounce a function
     *
     * @param func      function to executoe
     * @param wait      wait duration
     * @param immediate wait or immediate executue
     */
    var debounce = function (func, wait, immediate) {
        var timeout;
        var args;
        var context;
        var timestamp;
        var result;
        return function () {
            context = this;
            // eslint-disable-next-line prefer-rest-params
            args = arguments;
            timestamp = new Date();
            function later() {
                var now = new Date();
                var last = now.getTime() - timestamp.getTime();
                if (last < wait) {
                    timeout = setTimeout(later, wait - last);
                }
                else {
                    timeout = null;
                    if (!immediate) {
                        result = func.apply(context, args);
                    }
                }
            }
            var callNow = immediate && !timeout;
            if (!timeout) {
                timeout = setTimeout(later, wait);
            }
            if (callNow) {
                result = func.apply(context, args);
            }
            return result;
        };
    };

    /**
     * Debounce decorator
     *
     *  class MyClass {
     *    debounceable(10)
     *    myFn() { ... }
     *  }
     */
    function debounceable(duration, immediate) {
        // eslint-disable-next-line @typescript-eslint/explicit-module-boundary-types
        return function (_, key, descriptor) {
            return {
                configurable: true,
                enumerable: descriptor.enumerable,
                get: function getter() {
                    Object.defineProperty(this, key, {
                        configurable: true,
                        enumerable: descriptor.enumerable,
                        value: debounce(descriptor.value, duration, immediate)
                    });
                    // eslint-disable-next-line security/detect-object-injection
                    return this[key];
                }
            };
        };
    }

