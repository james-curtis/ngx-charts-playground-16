        JsonEditorNodeInfoComponent.prototype.updatePropertyName = function (name) {
            this.propertyNameChange.emit(name);
        };
        return JsonEditorNodeInfoComponent;
    }());
    JsonEditorNodeInfoComponent.decorators = [
