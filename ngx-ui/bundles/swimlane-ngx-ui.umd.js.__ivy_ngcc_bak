            if (!this.disabled) {
                this.pressed = true;
            }
        };
        LongPressButtonComponent.prototype.onLongPressFinish = function (e) {
            if (!this.disabled) {
                this.pressed = false;
                this.state = exports.LongPressButtonState.Submitted;
                this.longPress.emit(e);
                this.updateState();
            }
        };
