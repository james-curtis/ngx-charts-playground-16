            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ProgressSpinnerComponent.prototype, "percentage", {
            get: function () {
                return (100 / this._modeTotal) * this._modeValue;
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ProgressSpinnerComponent.prototype, "isComplete", {
