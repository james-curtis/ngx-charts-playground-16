        return ButtonModule;
    }());
    ButtonModule.decorators = [
        { type: i0.NgModule, args: [{
                    declarations: [FileButtonComponent, ButtonComponent],
                    exports: [FileButtonComponent, ng2FileUpload.FileUploadModule, ButtonComponent],
                    imports: [i2.CommonModule, ng2FileUpload.FileUploadModule]
