    var NavbarItemComponent = /** @class */ (function () {
        function NavbarItemComponent(_cdr, _el) {
            this._cdr = _cdr;
            this._el = _el;
            this.activeChange = new i0.EventEmitter();
        }
        Object.defineProperty(NavbarItemComponent.prototype, "active", {
            get: function () {
                return this._active;
            },
            set: function (v) {
                if (v !== this.active) {
                    this._active = coercion.coerceNumberProperty(v);
                    this.activeChange.emit(this._active);
                    this._cdr.markForCheck();
                }
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(NavbarItemComponent.prototype, "total", {
            get: function () {
                return this._total;
            },
            set: function (v) {
                this._total = coercion.coerceNumberProperty(v);
                this._cdr.markForCheck();
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(NavbarItemComponent.prototype, "index", {
            get: function () {
                return this._index;
            },
            set: function (v) {
                this._index = coercion.coerceNumberProperty(v);
                this._cdr.markForCheck();
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(NavbarItemComponent.prototype, "width", {
            get: function () {
                return this._el.nativeElement.clientWidth;
            },
            enumerable: false,
            configurable: true
        });
        NavbarItemComponent.prototype.setActive = function () {
            this.active = this.index;
        };
        return NavbarItemComponent;
    }());
