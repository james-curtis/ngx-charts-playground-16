    var SchemaValidatorService = /** @class */ (function () {
        function SchemaValidatorService() {
            this.ajv = new Ajv__default['default']({
                allErrors: true
            });
            this.ajv.addFormat('password', '.*');
            this.ajv.addFormat('code', '.*');
        }
        /**
         * Validates schemas of a specified type
         */
        SchemaValidatorService.prototype.validate = function (schema, model) {
            var valid = this.ajv.validate(schema, model);
            if (!valid) {
                return this.ajv.errors;
            }
            else
                return [];
        };
        return SchemaValidatorService;
