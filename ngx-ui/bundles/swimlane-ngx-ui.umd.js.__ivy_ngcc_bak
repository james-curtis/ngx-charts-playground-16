    var StepperModule = /** @class */ (function () {
        function StepperModule() {
        }
        return StepperModule;
    }());
    StepperModule.decorators = [
        { type: i0.NgModule, args: [{
                    imports: [i2.CommonModule, ResizeObserverModule],
                    declarations: [StepperComponent, StepComponent, StepContentDirective],
                    exports: [StepperComponent, StepComponent, StepContentDirective]
                },] }
