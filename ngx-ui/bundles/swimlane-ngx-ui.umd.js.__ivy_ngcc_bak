    CardAvatarComponent.propDecorators = {
        src: [{ type: i0.Input }],
        status: [{ type: i0.Input }]
    };

    var CardPlaceholderSize;
    (function (CardPlaceholderSize) {
        CardPlaceholderSize["Small"] = "small";
        CardPlaceholderSize["Medium"] = "medium";
        CardPlaceholderSize["Large"] = "large";
    })(CardPlaceholderSize || (CardPlaceholderSize = {}));
    var CardPlaceholderComponent = /** @class */ (function () {
        function CardPlaceholderComponent() {
            this.size = CardPlaceholderSize.Medium;
            this.CardPlaceholderSize = CardPlaceholderSize;
        }
        return CardPlaceholderComponent;
    }());
    CardPlaceholderComponent.decorators = [
        { type: i0.Component, args: [{
                    exportAs: 'ngxCardPlaceholder',
                    selector: 'ngx-card-placeholder',
                    template: '',
                    host: {
                        class: 'ngx-card-placeholder',
                        '[class.small]': 'size === CardPlaceholderSize.Small',
                        '[class.medium]': 'size === CardPlaceholderSize.Medium',
                        '[class.large]': 'size === CardPlaceholderSize.Large'
                    },
