    function partsToStyle(parts) {
        var _a = __read(parts, 3), grow = _a[0], shrink = _a[1], basis = _a[2];
        return grow + " " + shrink + " " + basis;
    }
    function basisToParts(grow, shrink, flexBasis) {
        // TODO: validate?
        var parts = flexBasis.split(' ');
        if (parts.length === 3) {
            return parts;
        }
        return [grow, shrink, flexBasis];
    }

    var DEFAULT_BASIS$1 = '1 1 1e-9px';
    function validateBasis(basis, grow, shrink) {
        if (grow === void 0) { grow = '1'; }
        if (shrink === void 0) { shrink = '1'; }
        var parts = [grow, shrink, basis];
        var matches = basis.split(' ');
        if (matches.length === 3) {
            return matches;
        }
        return parts;
    }
    var SplitAreaDirective = /** @class */ (function () {
