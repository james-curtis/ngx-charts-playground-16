    /* eslint-disable @typescript-eslint/no-non-null-assertion */
    var AutoSizeInputDirective = /** @class */ (function () {
        function AutoSizeInputDirective(element, ngModel, renderer) {
            this.element = element;
            this.ngModel = ngModel;
            this.renderer = renderer;
            this.extraWidth = 0;
            this.includeBorders = false;
            this.includePadding = true;
            this.includePlaceholder = true;
            this.maxWidth = -1;
            this.minWidth = -1;
            this.setParentWidth = false;
        }
        Object.defineProperty(AutoSizeInputDirective.prototype, "borderWidth", {
            get: function () {
                return this.includeBorders ? 2 * this._getPropertyWidth('border') : 0;
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(AutoSizeInputDirective.prototype, "paddingWidth", {
            get: function () {
                return this.includePadding ? this._getPropertyWidth('padding-left') + this._getPropertyWidth('padding-right') : 0;
            },
            enumerable: false,
            configurable: true
        });
        AutoSizeInputDirective.prototype.ngAfterContentChecked = function () {
            this.updateWidth();
        };
        AutoSizeInputDirective.prototype.ngAfterViewInit = function () {
            var _this = this;
            if (this.ngModel) {
                this.ngModel.valueChanges
                    .pipe(operators.filter(function (val) { return !!val; }), operators.take(1))
                    .subscribe(function () { return _this.updateWidth(); });
            }
        };
        // eslint-disable-next-line @typescript-eslint/member-ordering
        AutoSizeInputDirective.prototype.onInput = function () {
            this.updateWidth();
        };
        AutoSizeInputDirective.prototype.setWidth = function (width) {
            var element = this.element.nativeElement;
            var parent = this.renderer.parentNode(element);
            // eslint-disable-next-line @typescript-eslint/no-unused-expressions
            this.setParentWidth
                ? this.renderer.setStyle(parent, 'width', width + 'px')
                : this.renderer.setStyle(element, 'width', width + 'px');
        };
        AutoSizeInputDirective.prototype.setWidthUsingText = function (text) {
            this.setWidth(this.textWidth(text) + this.extraWidth + this.borderWidth + this.paddingWidth);
        };
        AutoSizeInputDirective.prototype.textWidth = function (value) {
            var ctx = this.renderer.createElement('canvas').getContext('2d');
            var style = window.getComputedStyle(this.element.nativeElement, '');
            var fontStyle = style.getPropertyValue('font-style');
            var fontVariant = style.getPropertyValue('font-variant');
            var fontWeight = style.getPropertyValue('font-weight');
            var fontSize = style.getPropertyValue('font-size');
            var fontFamily = style.getPropertyValue('font-family');
            // font string format: {normal, normal, 700, 20px, Roboto, "Helvetica Neue", sans-serif}
            ctx.font = fontStyle + ' ' + fontVariant + ' ' + fontWeight + ' ' + fontSize + ' ' + fontFamily;
            return ctx.measureText(value).width;
        };
        AutoSizeInputDirective.prototype.updateWidth = function () {
            var inputText = this.ngModel ? this.ngModel.value : this._getProperty('value');
            var placeHolderText = this._getProperty('placeholder');
            var inputTextWidth = this.textWidth(inputText) + this.extraWidth + this.borderWidth + this.paddingWidth;
            var setMinWidth = this.minWidth > 0 && this.minWidth > inputTextWidth;
            var setPlaceHolderWidth = this.includePlaceholder &&
                placeHolderText.length > 0 &&
                this.textWidth(placeHolderText) > this.textWidth(inputText);
            var setMaxWidth = this.maxWidth > 0 && this.maxWidth < inputTextWidth;
            if (setMinWidth) {
                this.setWidth(this.minWidth);
            }
            else if (setPlaceHolderWidth) {
                this.setWidthUsingText(placeHolderText);
            }
            else if (setMaxWidth) {
                this.setWidth(this.maxWidth);
            }
            else {
                this.setWidthUsingText(inputText);
            }
        };
        AutoSizeInputDirective.prototype._getProperty = function (property) {
            try {
                return this.element.nativeElement[property];
            }
            catch (error) {
                return '';
            }
        };
        AutoSizeInputDirective.prototype._getPropertyWidth = function (property) {
            var width = window.getComputedStyle(this.element.nativeElement, '').getPropertyValue(property);
            return parseInt(width, 10);
        };
        return AutoSizeInputDirective;
    }());
    AutoSizeInputDirective.decorators = [
        { type: i0.Directive, args: [{
                    selector: '[autoSizeInput]'
                },] }
    ];
    AutoSizeInputDirective.ctorParameters = function () { return [
        { type: i0.ElementRef },
        { type: forms.NgModel, decorators: [{ type: i0.Optional }] },
        { type: i0.Renderer2 }
    ]; };
    AutoSizeInputDirective.propDecorators = {
        extraWidth: [{ type: i0.Input }],
        includeBorders: [{ type: i0.Input }],
        includePadding: [{ type: i0.Input }],
        includePlaceholder: [{ type: i0.Input }],
        maxWidth: [{ type: i0.Input }],
        minWidth: [{ type: i0.Input }],
        setParentWidth: [{ type: i0.Input }],
        onInput: [{ type: i0.HostListener, args: ['input',] }]
    };

    var AutoSizeInputModule = /** @class */ (function () {
        function AutoSizeInputModule() {
        }
        return AutoSizeInputModule;
    }());
    AutoSizeInputModule.decorators = [
        { type: i0.NgModule, args: [{
                    imports: [i2.CommonModule],
                    declarations: [AutoSizeInputDirective],
                    exports: [AutoSizeInputDirective]
                },] }
    ];

    /* eslint-disable security/detect-object-injection */
    var cache = {};
    /**
     * Generates a short id.
     *
     * Description:
     * 	A 5-character alphanumeric sequence (364 = 1.6 million)
     * 	This should only be used for JavaScript specific models.
     * 	http://stackoverflow.com/questions/6248666/how-to-generate-short-uid-like-ax4j9z-in-js
     *
     * 	Example: `aebgf`
     */
    var id = function () {
        // eslint-disable-next-line no-bitwise
        var newId = ('0000' + ((Math.random() * Math.pow(36, 4)) << 0).toString(36)).slice(-4);
        // append a 'a' because neo gets mad
        newId = "a" + newId;
        // ensure not already used
        /* istanbul ignore else */
        if (!cache[newId]) {
            cache[newId] = true;
            return newId;
        }
        /* istanbul ignore next */
        return id();
    };

