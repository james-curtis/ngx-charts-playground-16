    var AutosizeDirective = /** @class */ (function () {
        function AutosizeDirective(element) {
            this.element = element;
            this._enabled = false;
        }
        Object.defineProperty(AutosizeDirective.prototype, "enabled", {
            get: function () {
                return this._enabled;
            },
            set: function (v) {
                this._enabled = coercion.coerceBooleanProperty(v);
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(AutosizeDirective.prototype, "nodeName", {
            get: function () {
                return this.element.nativeElement.nodeName;
            },
            enumerable: false,
            configurable: true
        });
        AutosizeDirective.prototype.onInput = function () {
            if (this._enabled) {
                var nativeEl = this.element.nativeElement;
                if (this.nodeName === 'TEXTAREA') {
                    nativeEl.style.height = 'auto';
                    if (nativeEl.clientHeight < nativeEl.scrollHeight) {
                        nativeEl.style.height = nativeEl.scrollHeight + "px";
                    }
                }
                else {
                    nativeEl.style.width = 'auto';
                    if (nativeEl.clientWidth < nativeEl.scrollWidth) {
                        nativeEl.style.width = nativeEl.scrollWidth + "px";
                    }
                }
            }
        };
        return AutosizeDirective;
    }());
