            enumerable: false,
            configurable: true
        });
        InputComponent.prototype.ngAfterViewInit = function () {
            var _this = this;
            if (this.autofocus) {
                setTimeout(function () {
                    _this.element.nativeElement.focus();
                    // sometimes the label doesn't update on load
                    _this.cdr.markForCheck();
                });
            }
        };
        InputComponent.prototype.ngOnDestroy = function () {
            this.clearSpinnerInterval();
        };
        InputComponent.prototype.onChange = function (event) {
            event.stopPropagation();
            this.change.emit(this.value);
        };
        InputComponent.prototype.onKeyUp = function (event) {
            event.stopPropagation();
            this.keyup.emit(event);
        };
        InputComponent.prototype.onFocus = function (event) {
            var _this = this;
            event.stopPropagation();
            this.focused = true;
            if (this.autoSelect) {
                setTimeout(function () { return _this.element.nativeElement.select(); });
                this.select.emit(event);
            }
            this.focus.emit(event);
            this.onTouchedCallback();
        };
        InputComponent.prototype.onBlur = function (event) {
            event.stopPropagation();
            this.focused = false;
            this.blur.emit(event);
        };
        InputComponent.prototype.validate = function (c) {
            if (this.type !== exports.InputTypes.number) {
                return null;
            }
            return Object.assign(Object.assign({}, forms.Validators.max(this.max)(c)), forms.Validators.min(this.min)(c));
        };
        InputComponent.prototype.writeValue = function (val) {
            if (val !== this._value) {
                this._value = val;
            }
            this.cdr.markForCheck();
        };
        InputComponent.prototype.registerOnChange = function (fn) {
            this.onChangeCallback = fn;
        };
        InputComponent.prototype.registerOnTouched = function (fn) {
            this.onTouchedCallback = fn;
        };
        InputComponent.prototype.togglePassword = function () {
            this.passwordTextVisible = !this.passwordTextVisible;
            this.element.nativeElement.focus();
        };
        InputComponent.prototype.setDisabledState = function (isDisabled) {
            this.disabled = coercion.coerceBooleanProperty(isDisabled);
        };
        InputComponent.prototype.incrementValue = function (event) {
            var _this = this;
            this.increment(event);
            if (!this._spinnerInterval) {
                this._spinnerTimeout = setTimeout(function () {
                    _this._spinnerInterval = setInterval(function () {
                        _this.increment(event);
                    }, 50);
                }, 500);
            }
        };
        InputComponent.prototype.decrementValue = function (event) {
            var _this = this;
            this.decrement(event);
            if (!this._spinnerInterval) {
                this._spinnerTimeout = setTimeout(function () {
                    _this._spinnerInterval = setInterval(function () {
                        _this.decrement(event);
                    }, 50);
                }, 500);
            }
        };
        InputComponent.prototype.clearSpinnerInterval = function () {
            clearTimeout(this._spinnerTimeout);
            this._spinnerTimeout = undefined;
            clearInterval(this._spinnerInterval);
            this._spinnerInterval = undefined;
        };
        InputComponent.prototype.updateInputType = function () {
            // eslint-disable-next-line
            this.type$.next(this.passwordTextVisible && exports.InputTypes.password === this.type ? exports.InputTypes.text : this.type);
        };
        InputComponent.prototype.increment = function (event) {
            event.preventDefault();
            if (!this.disabled) {
                var el = this.element.nativeElement;
                var max = +this.max;
                if ((max || max === 0) && +el.value >= max)
                    return;
                el.value = el.value ? (+el.value + 1).toString() : '1';
                this.value = el.value;
                this.change.emit(this._value);
                if (document.activeElement !== this.inputControl.nativeElement) {
                    this.inputControl.nativeElement.focus();
                }
            }
        };
        InputComponent.prototype.decrement = function (event) {
            event.preventDefault();
            if (!this.disabled) {
                var el = this.element.nativeElement;
                var min = +this.min;
                if (min || min === 0) {
                    if (min === 0 && !el.value) {
                        el.value = '0';
                        this.value = el.value;
                        this.change.emit(this._value);
                        this.inputControl.nativeElement.focus();
                        return;
                    }
                    else if (+el.value <= min) {
                        return;
                    }
                }
                el.value = el.value ? (+el.value - 1).toString() : '-1';
                this.value = el.value;
                this.change.emit(this._value);
                if (document.activeElement !== this.inputControl.nativeElement) {
                    this.inputControl.nativeElement.focus();
                }
            }
        };
        return InputComponent;
