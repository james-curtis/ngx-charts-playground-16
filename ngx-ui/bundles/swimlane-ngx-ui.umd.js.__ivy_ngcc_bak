    var ObjectValuesPipe = /** @class */ (function () {
        function ObjectValuesPipe() {
        }
        ObjectValuesPipe.prototype.transform = function (object) {
            return Object.values(object);
        };
        return ObjectValuesPipe;
    }());
    ObjectValuesPipe.decorators = [
        { type: i0.Pipe, args: [{ name: 'objectValues' },] }
    ];

    var JsonEditorNodeInfoComponent = /** @class */ (function () {
        function JsonEditorNodeInfoComponent() {
