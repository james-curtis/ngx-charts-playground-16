    var ProgressSpinnerComponent = /** @class */ (function () {
        function ProgressSpinnerComponent(_cdr) {
            this._cdr = _cdr;
            this.mode = exports.ProgressSpinnerMode.Indeterminate;
            this.color = '#1483FF';
            this.failStatusColor = '#FF4514';
            this.appearance = exports.SpinnerAppearance.Default;
            this.uid = id();
            this.ProgressSpinnerMode = exports.ProgressSpinnerMode;
            this._value = 0;
            this._total = 100;
            this._diameter = 100;
            this._strokeWidth = 3;
            this._indeterminate = {
                value: 50,
                total: 100
            };
        }
        Object.defineProperty(ProgressSpinnerComponent.prototype, "value", {
            get: function () {
                return this._value;
            },
