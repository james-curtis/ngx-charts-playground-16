    exports.ButtonState = void 0;
    (function (ButtonState) {
        ButtonState["InProgress"] = "in-progress";
        ButtonState["Active"] = "active";
        ButtonState["Success"] = "success";
        ButtonState["Fail"] = "fail";
    })(exports.ButtonState || (exports.ButtonState = {}));

    var ButtonComponent = /** @class */ (function () {
        function ButtonComponent() {
            this.inProgress$ = new rxjs.BehaviorSubject(false);
            this.active$ = new rxjs.BehaviorSubject(false);
            this.success$ = new rxjs.BehaviorSubject(false);
            this.fail$ = new rxjs.BehaviorSubject(false);
            this._state = exports.ButtonState.Active;
            this._disabled = false;
        }
        Object.defineProperty(ButtonComponent.prototype, "disabled", {
            get: function () {
                return this._disabled;
            },
            set: function (v) {
                this._disabled = coercion.coerceBooleanProperty(v);
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ButtonComponent.prototype, "state", {
            get: function () {
                return this._state;
            },
            set: function (v) {
                this._state = v;
                this.inProgress$.next(v === exports.ButtonState.InProgress);
                this.active$.next(v === exports.ButtonState.Active);
                this.success$.next(v === exports.ButtonState.Success);
                this.fail$.next(v === exports.ButtonState.Fail);
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ButtonComponent.prototype, "timeout", {
            get: function () {
                return this._timeout === undefined ? 3000 : this._timeout;
            },
            set: function (v) {
                this._timeout = coercion.coerceNumberProperty(v);
            },
            enumerable: false,
            configurable: true
        });
        ButtonComponent.prototype.ngOnInit = function () {
            this.updateState();
        };
        ButtonComponent.prototype.ngOnChanges = function () {
            this.updateState();
            this.updatePromise();
        };
        ButtonComponent.prototype.updatePromise = function () {
            var _this = this;
            if (this.promise) {
                this.state = exports.ButtonState.InProgress;
                return this.promise
                    .then(function () {
                    _this.state = exports.ButtonState.Success;
                    _this.updateState();
                })
                    .catch(function () {
                    _this.state = exports.ButtonState.Fail;
                    _this.updateState();
                });
            }
        };
        ButtonComponent.prototype.updateState = function () {
            var _this = this;
            if (!this.state) {
                this.state = exports.ButtonState.Active;
            }
            if (this.timeout &&
                (this.state === exports.ButtonState.Success || this.state === exports.ButtonState.Fail || this.state === exports.ButtonState.InProgress)) {
                clearTimeout(this._timer);
                this._timer = setTimeout(function () {
                    _this.state = exports.ButtonState.Active;
                    _this.updateState();
                }, this.timeout);
            }
        };
        ButtonComponent.prototype.onClick = function (event) {
            if (this.disabled) {
                event.stopPropagation();
                event.preventDefault();
                return false;
            }
            return true;
        };
        return ButtonComponent;
    }());
    ButtonComponent.decorators = [
        { type: i0.Component, args: [{
                    selector: 'ngx-button',
                    exportAs: 'ngxButton',
                    template: "<button [disabled]=\"disabled\">\n  <span class=\"content\"><ng-content></ng-content></span>\n  <span class=\"state-icon\">\n    <span *ngIf=\"inProgress$ | async\" class=\"icon icon-loading\"></span>\n    <span *ngIf=\"success$ | async\" class=\"icon icon-check\"></span>\n    <span *ngIf=\"fail$ | async\" class=\"icon icon-x\"></span>\n  </span>\n</button>\n",
                    host: {
                        class: 'ngx-button',
                        '[class.in-progress]': 'inProgress$.value',
                        '[class.active]': 'active$.value',
                        '[class.success]': 'success$.value',
                        '[class.fail]': 'fail$.value',
                        '[class.disabled-button]': 'disabled'
                    },
                    encapsulation: i0.ViewEncapsulation.None,
                    changeDetection: i0.ChangeDetectionStrategy.OnPush,
